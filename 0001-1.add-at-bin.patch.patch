+static void cipserver_close(void)
+{
+    ln_trans_sys_stat_handle_t p = ln_get_tcpip_handle();
+
+    if (p->ServerSock > -1)
+    {
+        Log_i("close server socket:%d!\r\n", p->ServerSock);
+        //shutdown(p->ServerSock, SHUT_RDWR);
+        close(p->ServerSock);
+        p->ServerSock = -1;
+		OS_ThreadDelete(&g_cipserver_thread);
+    }
+}
