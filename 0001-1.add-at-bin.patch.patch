+			server_mode = 0; 
+		}
+		else if(strstr(server_type,"TCPv6") != NULL)
+		{
+			server_mode = 1; 
+		}
+		else if(strstr(server_type,"SSL") != NULL)
+		{
+			server_mode = 2; 
+		}
+		else if(strstr(server_type,"SSLv6") != NULL)
+		{
+			server_mode = 3; 
+		}
