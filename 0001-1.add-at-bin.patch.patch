+static int ln_at_drv_init_pwm(uint32_t freq, uint8_t duty_res, uint8_t channel_port , ln_at_pwm_channel_t pwm_channel)
+{
+	uint32_t reg_base = 0;
+	
+	ln_at_drv_pwm_item_t* pwm_hdl = ln_at_drv_pwm_handle();
