+    p->cStaStat = STA_TCPUDP;
+
+    ln_at_printf(LN_AT_RET_OK_STR);
+    return LN_AT_ERR_NONE;
+
+__err:
+    ln_at_printf(LN_AT_RET_ERR_STR);
